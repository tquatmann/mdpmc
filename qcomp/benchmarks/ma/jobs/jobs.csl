"completiontime": Tmin=? [ F "all_jobs_finished"]; // Expected time until all jobs are finished
"avgtime": R{"avg_waiting_time"}max=? [ F "all_jobs_finished"]; // Expected time that a job requires to complete (on average)
"prhalfdone": Pmax=? [ F<=(N/(4*K)) "half_of_jobs_finished"]; // Probability that half of the jobs are finished within N/(4k) time units